      {
        "id": "HealthcareService.name",
        "path": "HealthcareService.name",
        "mustSupport": true
      },
      {
        "id": "HealthcareService.telecom",
        "path": "HealthcareService.telecom",
        "mustSupport": true
      },
      {
        "id": "HealthcareService.telecom.system",
        "path": "HealthcareService.telecom.system",
        "mustSupport": true
      },
