        "max": "*",
        "type": [
          {
            "code": "Extension",
            "profile": [
              "https://gematik.de/fhir/directory/StructureDefinition/ServiceCoveragePostalCode"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id": "HealthcareService.characteristic",
        "path": "HealthcareService.characteristic",
